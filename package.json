{
    "name": "notion-publish-tool",
    "version": "0.0.1",
    "description": "Post to Mitchinson.dev from a notion database.",
    "main": "dist/main.js",
    "scripts": {
        "prestart": "npm run build",
        "start": "node dist/main.js",
        "build": "tsc & npm run sveltebuild",
        "sveltebuild": "cd src/svelte && rollup -c",
        "postbuild": "cp -r frontend dist",
        "watch": "nodemon src/main.ts -e js,ts,svelte -x 'npm run start' -i dist -i frontend",
        "test": "jest --coverage",
        "view:cov": "serve coverage/lcov-report",
        "clean": "rm -rf coverage dist",
        "lint": "eslint 'src/**/*.ts'",
        "lint:fix": "eslint 'src/**/*.ts' --fix",
        "test:watch": "jest --coverage --watchAll",
        "check": "svelte-check --tsconfig src/svelte/tsconfig.json"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/jest": "^25.2.3",
        "@types/node": "^14.0.11",
        "@types/source-map-support": "^0.5.1",
        "@typescript-eslint/eslint-plugin": "^3.1.0",
        "@typescript-eslint/parser": "^3.1.0",
        "eslint": "^7.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "express": "^4.17.2",
        "jest": "^26.0.1",
        "onchange": "^7.0.2",
        "prettier": "^2.0.5",
        "serve": "^11.3.2",
        "source-map-support": "^0.5.19",
        "ts-jest": "^26.1.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "concurrently": "^7.0.0",
        "jest-junit": "^13.0.0",
        "nodemon": "^2.0.13",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.0.0",
        "svelte-check": "^2.0.0",
        "svelte-preprocess": "^4.0.0",
        "@rollup/plugin-typescript": "^8.0.0",
        "typescript": "^4.0.0",
        "tslib": "^2.0.0",
        "@tsconfig/svelte": "^2.0.0"
    }
}

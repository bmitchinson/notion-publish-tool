{
    "name": "notion-publish-tool",
    "version": "0.0.1",
    "description": "Post to Mitchinson.dev from a notion database.",
    "main": "dist/main.js",
    "scripts": {
        "prestart": "npm run build",
        "start": "node dist/main.js",
        "build": "tsc & npm run sveltebuild",
        "sveltebuild": "cd src/svelte && rollup -c",
        "postbuild": "cp -r frontend dist",
        "watch": "nodemon src/main.ts -e js,ts,svelte -x 'npm run start' -i dist -i frontend",
        "test": "jest --coverage",
        "view:cov": "serve coverage/lcov-report",
        "clean": "rm -rf coverage dist",
        "lint": "eslint 'src/**/*.ts'",
        "lint:fix": "eslint 'src/**/*.ts' --fix",
        "test:watch": "jest --coverage --watchAll",
        "check": "svelte-check --tsconfig src/svelte/tsconfig.json",
        "prepare": "husky install",
        "heroku:log": "heroku logs --tail -a bmitchinson-notion-publisher",
        "storybook": "start-storybook -p 6006",
        "storybookbuild": "build-storybook -o storybook_build"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "@rollup/plugin-typescript": "^8.0.0",
        "@tsconfig/svelte": "^2.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^25.2.3",
        "@types/node": "^14.0.11",
        "@types/source-map-support": "^0.5.1",
        "@types/ws": "^8.2.2",
        "@typescript-eslint/eslint-plugin": "^3.1.0",
        "@typescript-eslint/parser": "^3.1.0",
        "concurrently": "^7.0.0",
        "eslint": "^7.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "express": "^4.17.2",
        "jest": "^26.0.1",
        "jest-junit": "^13.0.0",
        "nodemon": "^2.0.13",
        "onchange": "^7.0.2",
        "prettier": "^2.0.5",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "serve": "^11.3.2",
        "source-map-support": "^0.5.19",
        "svelte": "^3.0.0",
        "svelte-check": "^2.0.0",
        "svelte-preprocess": "^4.0.0",
        "ts-jest": "^26.1.0",
        "ts-node": "^8.10.2",
        "tslib": "^2.0.0",
        "typescript": "^3.9.5",
        "ws": "^8.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-interactions": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/builder-webpack4": "^6.5.9",
        "@storybook/manager-webpack4": "^6.5.9",
        "@storybook/storybook-deployer": "^2.8.11",
        "@storybook/svelte": "^6.5.9",
        "@storybook/testing-library": "^0.0.13",
        "babel-loader": "^8.2.5",
        "eslint-plugin-storybook": "^0.5.13",
        "husky": "^7.0.0",
        "svelte-loader": "^3.1.3"
    }
}
